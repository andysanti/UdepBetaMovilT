<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.DetalleAsistenciaActivity">

    <TextView
        android:id="@+id/lblSesion"
        android:layout_width="178dp"
        android:layout_height="44dp"
        android:text="TextView"
        android:gravity="center"
        app:layout_constraintRight_toLeftOf="@+id/lblTipoAsistencia"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/lblTipoAsistencia"
        android:layout_width="204dp"
        android:layout_height="45dp"
        android:text="TextView"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@+id/lblSesion"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>